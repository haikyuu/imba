  "browser": {
    ".": "./dist/imba.mjs",
    "./program": "./program.imba.js",
    "./compiler": "./compiler.imba.js"
  },
  "exports": {
    ".": {
      "browser": "./dist/imba.mjs",
      "default": "./index.js"
    },
    "./program": "./program.imba.js",
    "./workers": "./workers.imba.js",
    "./loader": "./loader.imba.js",
    "./compiler": "./compiler.imba.js"
  },
